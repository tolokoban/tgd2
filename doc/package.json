        "@tolokoban/ui": "^0.19.3",
        "colord": "^2.9.3",
        "highlight.js": "^11.10.0",
        "prismjs": "^1.29.0",
        "react": "18.3.1",
        "react-dom": "18.3.1"
    },
    "devDependencies": {
        "@mdx-js/loader": "^3.0.1",
        "@types/mdx": "^2.0.13",
        "@types/node": "^22.5.4",
        "@types/prismjs": "^1.26.4",
        "@types/react": "^18.3.5",
        "@types/react-dom": "^18.3.0",
        "babel-loader": "^9.1.3",
        "build-react-routes": "^0.10.3",
        "clean-webpack-plugin": "^4.0.0",

        "remark-images": "^4.0.0",
        "style-loader": "^3.3.4",
        "ts-loader": "^9.5.1",
        "tsx": "^4.19.1",
        "typescript": "5.6.2",
        "webpack": "^5.94.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.1.0",
        "workbox-webpack-plugin": "^7.1.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",

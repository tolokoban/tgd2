{
    "version": "2.0.5",
    "private": true,
    "homepage": "https://tolokoban.github.io/tgd2",
    "scripts": {
        "prebuild": "npm run demo && build-react-routes ./src/app/",
        "build": "webpack --mode=production",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d ./build",
        "prestart": "build-react-routes --after 'npm run demo' --watch ./src/app/ &",
        "start": "webpack serve --mode=development",
        "demo": "tsx ./scripts/demo/index.ts ./src/app"
    },
    "dependencies": {
        "@tolokoban/react-state": "^0.3.0",
        "@tolokoban/tgd": "file:..",
        "@tolokoban/type-guards": "^0.5.0",
        "@tolokoban/ui": "^0.17.10",
        "colord": "^2.9.3",
        "prismjs": "^1.29.0",
        "react": "18.3.1",
        "react-dom": "18.3.1"
    },
    "devDependencies": {
        "@mdx-js/loader": "^3.0.1",
        "@types/mdx": "^2.0.13",
        "@types/node": "^20.14.10",
        "@types/prismjs": "^1.26.4",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "babel-loader": "^9.1.3",
        "build-react-routes": "^0.10.3",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^6.11.0",
        "gh-pages": "^6.1.1",
        "html-webpack-plugin": "^5.6.0",
        "style-loader": "^3.3.4",
        "ts-loader": "^9.5.1",
        "tsx": "^4.16.2",
        "typescript": "5.5.3",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4",
        "workbox-webpack-plugin": "^7.1.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 3 chrome version",
            "last 3 firefox version",
            "last 3 safari version"
        ]
    },
    "port": 7133,
    "name": "@tolokoban/tgd",
    "description": "ToloGameDev library for WebGL2"
